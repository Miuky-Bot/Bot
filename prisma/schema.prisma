generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model guild {
  id         String    @id @unique(map: "Guild_id_key")
  created_at DateTime  @default(now())
  updated_at DateTime
  language   language?
  premium    premium?
  welcome    welcome?
}

model premium {
  id         String    @id @unique
  created_at DateTime  @default(now())
  ended_at   DateTime
  timestamp  DateTime?
  guildId    String    @unique
  guild      guild     @relation(fields: [guildId], references: [id])
}

model language {
  id         Int      @id @default(autoincrement())
  language   String   @default("en")
  created_at DateTime @default(now())
  updated_at DateTime
  guildId    String   @unique(map: "Language_guildId_key")
  guild      guild    @relation(fields: [guildId], references: [id], map: "Language_guildId_fkey")
}

model welcome {
  id                    Int      @id @default(autoincrement())
  enabled               Boolean?
  content               String?
  channelId             String?
  card_enabled          Boolean?
  card_background_color String?
  card_background_url   String?
  card_background_type  String?
  card_content          String?
  card_content_color    String?
  card_title_color      String?
  card_username_color   String?
  card_avatar_color     String?
  card_overlay          Float?
  created_at            DateTime @default(now())
  updated_at            DateTime
  guildId               String   @unique(map: "Welcome_guildId_key")
  role_enabled          Boolean?
  role_ids              Json?
  role_bot_ids          Json?
  guild                 guild    @relation(fields: [guildId], references: [id], map: "Welcome_guildId_fkey")
}
